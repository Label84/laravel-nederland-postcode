{
    "name": "label84/laravel-nederland-postcode",
    "description": "Integrate Dutch address validations into your Laravel application using the Nederland Postcode API.",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Label84",
        "email": "info@label84.com"
      }
    ],
    "require": {
        "label84/php-nederland-postcode": "^1.1"
    },
    "require-dev": {
        "laravel/pint": "^1.26",
        "orchestra/testbench": "^10.8",
        "larastan/larastan": "^3.8",
        "phpunit/phpunit": "^11.5|^12.5"
    },
    "autoload": {
        "psr-4": {
            "Label84\\NederlandPostcode\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Label84\\NederlandPostcode\\Laravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Label84\\NederlandPostcode\\Laravel\\NederlandPostcodeServiceProvider"
            ],
            "aliases": {
                "NederlandPostcode": "Label84\\NederlandPostcode\\Laravel\\Facades\\NederlandPostcode"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
